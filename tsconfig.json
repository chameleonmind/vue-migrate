{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    "noImplicitAny": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitReturns": false,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
